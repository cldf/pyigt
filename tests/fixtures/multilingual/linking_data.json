{
  "@context": "http://www.w3.org/ns/csvw",
  "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#Generic",
  "dc:title": "Examples for linking morphemes",
  "dc:license": "http://creativecommons.org/licenses/by/4.0/",
  "tables": [
	  {
	      "url": "linking_examples.csv",
	      "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#ExampleTable",
	      "tableSchema": {
	          "columns": [
	              {
	                  "name": "ID",
	                  "required": true,
	                  "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#id",
	                  "datatype": {
	                      "base": "string",
	                      "format": "[a-zA-Z0-9_\\-]+"
	                  },
					  "titles": "Example_ID"
	              },
	              {
	                  "name": "Language_ID",
	                  "required": true,
	                  "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#languageReference",
	                  "datatype": "string"
	              },
	              {
	                  "name": "Primary_Text",
	                  "required": true,
	                  "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#primaryText",
	                  "datatype": "string",
					  "titles": "Sentence"
	              },
	              {
	                  "name": "Analyzed_Word",
	                  "required": false,
	                  "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#analyzedWord",
	                  "datatype": "string",
	                  "separator": " ",
					  "titles": "Segmentation"
	              },
	              {
	                  "name": "Gloss",
	                  "required": true,
	                  "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#gloss",
	                  "datatype": "string",
	                  "separator": " "
	              },
	              {
	                  "name": "Translated_Text",
	                  "required": true,
	                  "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#translatedText",
	                  "datatype": "string",
					  "titles": "Translation"
	              },
                  {
                      "name": "Source",
                      "required": false,
                      "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#source",
                      "datatype": {
                          "base": "string"
                      }
                  },
	              {
	                  "name": "Meta_Language_ID",
	                  "required": false,
	                  "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#metaLanguageReference",
	                  "datatype": "string"
	              },
	              {
	                  "name": "Comment",
	                  "required": false,
	                  "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#comment",
	                  "datatype": "string"
	              }
	          ]
	      }
	  }
  ]
}